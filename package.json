    "appwrite": "^13.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.290.0",
    "next": "13",
    "prettier": "2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18",
    "react-hook-form": "^7.47.0",
    "react-intersection-observer": "^9.5.2",
    "tailwind-merge": "^1.14.0",
